	"figure":{
		"prefix":"fig",
		"body":["\\begin{figure}[hbt]",
					"\t\\centering",
					"\t\\includegraphics[width=0.5\\linewidth]{figname}",
					"\t\\caption{}",
				"\\end{figure}"
				],
		"description":"A template for figure env."
	},

	"table":{
		"prefix":"table 3-by-3",
		"body":["\\begin{table}[hbt]",
					"\t\\centering",
					"\t\\begin{tabular}{ccc} \\hline",
						"\t\tcol1 & col2 & col3 \\\\\\\\ \\hline",
						"\t\ta & a & a \\\\\\\\",
						"\t\ta & a & a \\\\\\\\",
						"\t\ta & a & a \\\\\\\\ \\hline",
					"\t\\end{tabular}",
					"\t\\caption{}",
				"\\end{table}"
				],
		"description":"A template for table env."
	},

	"equation":{
		"prefix":"eq",
		"body":["\\begin{align}",
					"\ta",
					"\t&=",
					"\tb + c \\\\\\\\",
					"\t&=",
					"\tc + d",
				"\\end{align}"
				],
		"description":"A template for equation env."
	}

}