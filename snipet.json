{
	"template-APS":{
		"prefix":"template",
		"body":["\\documentclass[aps,prd,preprint,superscriptaddress,nofootinbib]{revtex4-1}",
				"\n",
				"\\usepackage{amsmath, amssymb, latexsym, physics, mathrsfs, bm}",
				"\\usepackage{graphicx}",
				"\\usepackage{grffile}",
				"\\usepackage[",
					"\t colorlinks        = true,",
					"\t unicode           = true,",
					"\t pdfstartview      = FitV,",
					"\t linktocpage       = true,",
					"\t linkcolor         = OrangeRed,",
					"\t citecolor         = MediumSeaGreen,",
					"\t urlcolor          = RoyalBlue,",
					"\t bookmarks         = true,",
					"\t bookmarksnumbered = true,",
					"\t pdftitle={},",
					"\t pdfauthor={Shoichiro Tsutsui}",
					"]{hyperref}",
					"\\usepackage{ifthen}",
				"\n",	
				"%% If debugmode = true, show details of algebra.",
				"\\newboolean{debugmode}",
				"\\setboolean{debugmode}{true}",
				"\n",	
				"%% Numerical constats",
				"\\newcommand{\\e}{\\mathrm{e}}",
				"\\newcommand{\\im}{\\mathrm{i}\\mkern1mu}",
				"\n",	
				"%% Derivative",
				"\\newcommand{\\del}{\\partial}",
				"\n",	
				"%% Mathcal",
				"\\newcommand{\\calD}{\\mathcal{D}}",
				"\n",	
				"%% Hat (used for operator on Hilbert space)",
				"\\newcommand{\\hH}{\\hat{H}}",
				"\n",	
				"%% Mathematical symbols",
				"\\DeclareMathOperator{\\diag}{diag}",
				"\\DeclareMathOperator{\\Tprod}{\\mathrm{T}}",
				"\n",	
				"%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
				"\\begin{document}",
				"\n",					
				"\\title{Title}",
				"\n",	
				"\\author{Shoichiro Tsutsui}",
				"\\email[]{shoichiro.tsutsui@riken.jp}",
				"\\affiliation{",
					"\t Theoretical Research Division, Nishina Center, RIKEN, Wako, Saitama 351-0198, Japan",
				"}",				
				"\n",	
				"%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
				"\\begin{abstract}",
				"	Abstract here.",
				"\\end{abstract}",
				"\n",					
				"\\maketitle",
				"\\tableofcontents",
				"\n",	
				"%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
				"\\section{Introduction}",
				"\n",	
				"%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
				"\\section{Summary}",
				"\n",	
				"%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
				"\\section*{Acknowledgments}",
				"The authors are grateful to ... for fruitful discussion.",
				"S.\\ T.\\ was supported by the RIKEN Special Postdoctoral Researchers Program.",
				"\n",					
				"%\\bibliography{sample.bib}",
				"\n",					
				"\\end{document}",
				],
		"description":"A template for APS"
	},

